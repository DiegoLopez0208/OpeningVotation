"scripts": {
  "start:frontend": "cd frontend-animation-votation && npm start",
  "start:backend": "cd backend-animation-votation && npm start",
  "start": "concurrently -n \"Frontend,Backend\" -c \"blue,green\" \"npm run start:backend\" \"npm run start:frontend\"",
  "dev:frontend": "cd frontend-animation-votation && npm run dev",
  "dev:backend": "cd backend-animation-votation && npm run dev",
  "dev": "concurrently -n \"Backend,Frontend\" -c \"cyan,red\" \"npm run dev:backend\" \"npm run dev:frontend\""
},

